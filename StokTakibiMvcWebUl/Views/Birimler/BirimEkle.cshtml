@model StokTakibiEntities.Concrete.Birimler
@{
    ViewData["Title"] = "BirimEkle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<h4>Birim Ekle</h4>
<br />
<form method="post">
    <br />
    @Html.Label("Birim Adı")
    @Html.TextBoxFor(x => x.Birim, new { @class = "form-control" })
    @Html.ValidationMessageFor(x => x.Birim, "", new { @style = "color:red" })
    <br />
    @Html.Label("Açıklama")
    @Html.TextBoxFor(x => x.Aciklama, new { @class = "form-control" })
    <br />
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

@section scriptdanger
{
    
    <script src="~/Script-Dosyalari/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}